#include <stdio.h>
#swarag
main()
{
	long long int a[1100000],b[1100000],c,d,e,f,g,h,i,j,k,z[1100000],x=0,l;
	scanf("%lld",&c);
	for(d=1;d<=c;d++)
	{scanf("%lld",&a[d]);
		if(a[d]!=z[x])
		{x=x+1;
			z[x]=a[d];}}
	//swaragreddy
	scanf("%lld",&e);
	for(d=1;d<=e;d++)
	scanf("%lld",&b[d]);
	k=x;
	for(d=1;d<=e;d++)
	{j=0;
	for(i=k;i>=1;i--)
	{if(z[i]>b[d])
			  break;
			else
			k=k-1;}
		    
			printf("%lld\n",k+1);}}
